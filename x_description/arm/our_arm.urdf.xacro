       <geometry>  
         <cylinder length="0.075" radius="0.002"/>  
       </geometry>  
      <material name="white">
      </material> 
       <origin rpy="0 0 0" xyz="0 0 .0375"/>
	
    </visual>  
  </link> 


  <link name="finger2_link">  
    <visual>  
       <geometry>  
         <cylinder length="0.075" radius="0.002"/>  
       </geometry>  
      <material name="white">
      </material> 
       <origin rpy="0 0 0" xyz="0 0 .0375"/>
	
    </visual>  
  </link> 
