<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="test_robot"> 
<!--********************************const param***********************************-->
<xacro:property name="M_PI"   value="3.141592654"/>
<xacro:property name="M_PI_2" value="1.570796327"/>
<!--***********************************link***************************************-->
  <link name="arm_link"> 
      <visual> 
        <geometry>
         <mesh filename="package://x_description/arm/meshes/visual/mounting_base.stl" scale="0.0001 0.0001 0.0001"/>
         </geometry>
        <material name="white">
        <color rgba="1 1 1 1"/>
      </material>
       <origin rpy="${M_PI_2} 0 0" xyz="0 0 0"/>
      
    </visual>
    <collision> 
       <geometry>
         <mesh filename="package://x_description/arm/meshes/collision/mounting_base.stl" scale="0.0001 0.0001 0.0001"/>
         </geometry>
       <origin rpy="${M_PI_2} 0 0" xyz="0 0 0"/>
    </collision>
  </link>

  <link name="arm_rota_link"> 
      <visual> 
        <geometry>
          <mesh filename="package://x_description/arm/meshes/visual/rotating_base.stl" scale="0.0001 0.0001 0.0001"/>
        </geometry>
        <material name="white"/>
        <origin rpy="${M_PI_2} 0 ${M_PI}" xyz="0 0 0"/>
      </visual>
      <collision> 
        <geometry>
          <mesh filename="package://x_description/arm/meshes/collision/rotating_base.stl" scale="0.0001 0.0001 0.0001"/>
        </geometry>
        <origin rpy="${M_PI_2} 0 ${M_PI}" xyz="0 0 0"/>
      </collision>

  </link>

  <link name="arm_a_link">  
    <visual>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/visual/arm_a.stl" scale="0.0001 0.0001 0.0001"/>
       </geometry>  
      <material name="white"/>
       <origin rpy="${M_PI_2} 0 ${M_PI}" xyz="0 0 0"/>  
    </visual> 
    <collision>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/collision/arm_a.stl" scale="0.0001 0.0001 0.0001"/>
       </geometry>  
       <origin rpy="${M_PI_2} 0 ${M_PI}" xyz="0 0 0"/>  
    </collision>   
  </link> 

  <link name="passive_a_link"> 
  </link> 

  <link name="arm_b_link">  
    <visual>  
       <geometry>  
         <mesh filename="package://x_description/arm/meshes/visual/arm_b.stl" scale="0.0001 0.0001 0.0001"/>
       </geometry>  
      <material name="white"/>
       <origin rpy="${M_PI_2} 0 ${M_PI}" xyz="0 0 0"/>
     </visual>  
     <collision>  
       <geometry>  
         <mesh filename="package://x_description/arm/meshes/collision/arm_b.stl" scale="0.0001 0.0001 0.0001"/>
       </geometry>  
       <origin rpy="${M_PI_2} 0 ${M_PI}" xyz="0 0 0"/>
     </collision>  
  </link> 

  <link name="arm_c_link">  
    <visual>  
       <geometry>  
         <mesh filename="package://x_description/arm/meshes/visual/arm_c.stl" scale="0.0001 0.0001 0.0001"/>  
       </geometry>  
       <origin rpy="0 ${M_PI} 0" xyz="0 0 0"/> 
	 <material name="white"/>
    </visual>  
    <collision>  
              <geometry>  
         <mesh filename="package://x_description/arm/meshes/collision/arm_c.stl" scale="0.0001 0.0001 0.0001"/>  
       </geometry>  
       <origin rpy="0 ${M_PI} 0" xyz="0 0 0"/> 
	 
    </collision>  
  </link> 

  <link name="arm_d_link">  
    <visual>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/visual/arm_d.stl" scale="0.0001 0.0001 0.0001"/>   
       </geometry>  
       <origin rpy="${M_PI/2} ${M_PI} 0" xyz="0 0 0"/>
<material name="white"/>
    </visual>  
    <collision>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/collision/arm_d.stl" scale="0.0001 0.0001 0.0001"/>   
       </geometry>  
       <origin rpy="${M_PI/2} ${M_PI} 0" xyz="0 0 0"/>
    </collision>  
  </link> 

  <link name="arm_hand_link">  
    <visual>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/visual/hand.stl" scale="0.0001 0.0001 0.0001"/>   
       </geometry>  
       <origin rpy="0 0 0" xyz="0 0 0"/>  
<material name="white"/>
    </visual>  
    <collision>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/collision/hand.stl" scale="0.0001 0.0001 0.0001"/>   
       </geometry>  
       <origin rpy="0 0 0" xyz="0 0 0"/>  
    </collision>
  </link> 

  <link name="arm_finger_a_link">  
    <visual>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/visual/finger.stl" scale="0.0001 0.0001 0.0001"/>   
       </geometry>  
       <origin rpy="0 ${M_PI} 0" xyz="0 0.1 0"/>  
<material name="white"/>
    </visual>  
    <collision>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/collision/finger.stl" scale="0.0001 0.0001 0.0001"/>   
       </geometry>  
       <origin rpy="0 ${M_PI} 0" xyz="0 0.1 0"/>  
    </collision>
  </link> 

  <link name="arm_finger_b_link">  
    <visual>  
       <geometry>  
          <mesh filename="package://x_description/arm/meshes/visual/finger.stl" scale="0.0001 0.0001 0.0001"/>   
       </geometry>  
       <origin rpy="0 0 0" xyz="0 0.1 0"/>  
<material name="white"/>
    </visual>  
    <collision>  
        <geometry>  
          <mesh filename="package://x_description/arm/meshes/collision/finger.stl" scale="0.0001 0.0001 0.0001"/>   
       </geometry>  
       <origin rpy="0 0 0" xyz="0 0.1 0"/>  
    </collision>
  </link> 



 <!-- 
  <link name="finger1_link">  
